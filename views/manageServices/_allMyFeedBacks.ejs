<div id="allMyFeedBacks" class="container">
  <ul class="nav nav-pills nav-justified mb-3" id="pills-tab" role="tablist">
    <li class="nav-item" role="presentation">
      <button
        class="nav-link active"
        id="pills-SentFeedbacks-tab"
        data-bs-toggle="pill"
        data-bs-target="#pills-SentFeedbacks"
        type="button"
        role="tab"
        aria-controls="pills-SentFeedbacks"
        aria-selected="true"
      >
        <h5><strong>Sent Feedbacks</strong></h5> 
      </button>
    </li>
    <li class="nav-item" role="presentation">
      <button
        class="nav-link"
        id="pills-ReceivedFeedbacks-tab"
        data-bs-toggle="pill"
        data-bs-target="#pills-ReceivedFeedbacks"
        type="button"
        role="tab"
        aria-controls="pills-ReceivedFeedbacks"
        aria-selected="false"
      >
        <h5><strong>Received Feedbacks</strong></h5> 
      </button>
    </li>
  </ul>
  <div class="tab-content" id="pills-tabContent">
    <div
      class="tab-pane fade show active"
      id="pills-SentFeedbacks"
      role="tabpanel"
      aria-labelledby="pills-SentFeedbacks-tab"
    >
      <%- include('../manageServices/_displaySentFeedBack')  %> 
    </div>
    <div
      class="tab-pane fade"
      id="pills-ReceivedFeedbacks"
      role="tabpanel"
      aria-labelledby="pills-ReceivedFeedbacks-tab"
    >
      <%- include('../manageServices/_displayReceivedFeedBack')  %> 
    </div>
  </div>
</div>
